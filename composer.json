{
    "name": "pinoniq/drupal-console-extend-plugin",
    "description": "Drupal Console Extend Plugin",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Drupal Console Contributors",
            "homepage": "https://github.com/pinoniq/drupal-console-extend-plugin/graphs/contributors"
        }
    ],
    "type": "composer-plugin",
    "extra": {
        "class": "Drupal\\Console\\Composer\\Plugin\\Extender"
    },
    "require": {
        "composer-plugin-api": "^1.0 || ^2.0",
        "composer/installers": "^1.2",
        "symfony/yaml": "^4.4|~5.0",
        "symfony/finder": "^4.4|~5.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {"Drupal\\Console\\Composer\\Plugin\\": "src"}
    }
}
